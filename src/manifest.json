{
  "manifest_version": 3,
  "name": "Penguin-Link",
  "version": "1.3.8",
  "description": "A chrome extension detects fake or unwanted sites based on similarty of user entries.",
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ],
  "background": { 
     "service_worker": "background.bundle.js" 

    },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34x34.png"
  },
  "icons": {
    "128": "mstile-128x128.png"
  },
 
  "web_accessible_resources": [
    {
      "resources": ["content.styles.css", "mstile-128x128.png", "icon-34x34.png"],
      "matches": []
    }
  ]
}
